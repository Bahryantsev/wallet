{
  "manifest_version": 3,
  "name": "Wallet",
  "version": "0.0.2",
  "action": { "default_popup": "index.html" },
  "permissions": ["scripting", "tabs", "activeTab", "storage", "alarms"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "background": {
    "service_worker": "background.ts"
  }
}
